# My Github account that I use in personal life
Host personal
	HostName github.com
	AddKeysToAgent yes
	IdentitiesOnly yes
	IdentityFile ~/.ssh/personal

# My Github account that I use in the company
Host work
	HostName github.com
	AddKeysToAgent yes
	IdentitiesOnly yes
	IdentityFile ~/.ssh/work


# Default key for all hosts
Host *
	IdentityFile ~/.ssh/work
